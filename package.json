    "uuid": "^8.3.2",
    "wait-on": "^6.0.0"
  },
  "browserslist": [
    "Electron >= 11"
  ],
  "keywords": [
    "git",
    "jenkins",
    "terminal",
    "manager",
    "multi",
    "switch",
    "developer",
    "kit",
    "devkit",
    "devkitty"
  ]
}