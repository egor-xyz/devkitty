{
  "name": "devkitty",
  "version": "1.1.30",
  "productName": "Devkitty",
  "author": {
    "name": "Egor Stronhin",
    "email": "info@devkitty.app",
    "url": "https://devkitty.app/"
  },
  "license": "MIT",
  "repository": "https://github.com/egor-xyz/devkitty",
  "description": "devkitty - swiss army knife for developers",
  "private": true,
  "homepage": "./",
  "main": "./electron/electron.js",
  "scripts": {
    "start:test": "concurrently \"npm run react:start\" \"npm run electron:start\"",
    "postinstall": "electron-builder install-app-deps",
    "start": "nf start -p 7777",
    "react:start": "react-app-rewired start",
    "electron:start": "node electron/start-electron",
    "electron": "ELECTRON_ENABLE_LOGGING=1 ELECTRON_DISABLE_SECURITY_WARNINGS=false electron .",
    "build": "npm-run-all clean-packages build:react build:mac:dist",
    "build:react": "react-app-rewired build",
    "build:mac:dist": "NO_SIGN=1 electron-builder --mac zip --config electron/electron-builder.json -c.mac.identity=null",
    "build:mac:sign-test": "electron-builder --mac zip --config electron/electron-builder.json",
    "build:win": "npm-run-all clean-packages build:react build:win:dist",
    "build:win:dist": "electron-builder --win --config electron/electron-builder.json",
    "build:all": "npm-run-all clean-packages build:react package:all:dist",
    "build:all:dist": "electron-builder --mac --win --config electron/electron-builder.json",
    "deploy": "npm-run-all clean-packages build:react deploy:dist",
    "deploy:dist": "electron-builder --mac --config electron/electron-builder.json -p always",
    "deploy:dist:all": "electron-builder --mac --win --config electron/electron-builder.json -p always",
    "service:build-icons": "electron-icon-maker -i build-assets/icon.png -o public",
    "clean-packages": "rm -rf ./packages",
    "check-compilation": "tsc --p . --noEmit --skipLibCheck"
  },
  "dependencies": {
    "@google-cloud/translate": "^6.2.4",
    "app-path": "^3.3.0",
    "axios": "^0.21.1",
    "electron-is-dev": "^2.0.0",
    "electron-log": "^4.3.5",
    "electron-settings": "^4.0.2",
    "electron-unhandled": "^3.0.2",
    "electron-updater": "^4.3.9",
    "electron-util": "^0.17.0",
    "electron-window-state": "^5.0.3",
    "fix-path": "^3.0.0",
    "fs-extra": "^10.0.0",
    "keytar": "7.6.0",
    "open": "^8.2.1"
  },
  "devDependencies": {
    "@blueprintjs/core": "^3.46.0",
    "@blueprintjs/icons": "^3.27.0",
    "@blueprintjs/popover2": "^0.11.0",
    "@blueprintjs/select": "^3.16.5",
    "@egor.xyz/eslint-config": "^0.0.8",
    "@fortawesome/fontawesome-svg-core": "^1.2.35",
    "@fortawesome/free-brands-svg-icons": "^5.15.3",
    "@fortawesome/free-solid-svg-icons": "^5.15.3",
    "@fortawesome/react-fontawesome": "^0.1.14",
    "@octokit/rest": "^18.6.4",
    "@octokit/types": "^6.17.3",
    "@testing-library/jest-dom": "^5.14.1",
    "@testing-library/react": "^12.0.0",
    "@testing-library/user-event": "^13.1.9",
    "@types/fs-extra": "^9.0.11",
    "@types/git-url-parse": "^9.0.0",
    "@types/jquery-param": "^1.0.2",
    "@types/lodash": "^4.14.170",
    "@types/node": "^15.12.5",
    "@types/react": "^17.0.11",
    "@types/react-dom": "^17.0.7",
    "@types/react-linkify": "^1.0.0",
    "@types/react-router": "^5.1.15",
    "@types/react-router-dom": "^5.1.7",
    "@types/styled-components": "^5.1.10",
    "@types/universal-analytics": "^0.4.4",
    "@types/uuid": "^8.3.0",
    "clsx": "^1.1.1",
    "dotenv": "^10.0.0",
    "electron": "^11.4.9",
    "electron-builder": "^22.11.7",
    "electron-icon-maker": "^0.0.5",
    "electron-notarize": "^1.0.0",
    "foreman": "^3.0.1",
    "framer-motion": "^4.1.17",
    "git-url-parse": "^11.5.0",
    "jquery-param": "^1.1.8",
    "lodash": "^4.17.21",
    "moment": "^2.29.1",
    "node-sass": "^5.0.0",
    "node-version": "^2.0.0",
    "normalize.css": "^8.0.1",
    "npm-run-all": "^4.1.5",
    "query-string": "^7.0.1",
    "react": "^17.0.2",
    "react-app-rewired": "^2.1.8",
    "react-dom": "^17.0.2",
    "react-ga": "^3.3.0",
    "react-hotkeys": "^2.0.0",
    "react-linkify": "^1.0.0-alpha",
    "react-router": "^5.2.0",
    "react-router-dom": "^5.2.0",
    "react-scripts": "^4.0.3",
    "simple-git": "^2.40.0",
    "styled-components": "^5.3.0",
    "typescript": ">=3.3.1 <4.2.0",
